<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="panel" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="Hospital.Controllers.Physicians.DiagnosisReportController">
    <children>
        <TabPane fx:id="tabpane" layoutX="5.0" layoutY="7.0" prefHeight="393.0" prefWidth="593.0"
                 tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="TYPED TEXT">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <TextArea editable="false" fx:id="resultText" layoutX="4.0" layoutY="11.0"
                                          prefHeight="341.0"
                                          prefWidth="583.0"/>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="SCANNED IMAGE">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <ImageView fitHeight="340.0" fitWidth="583.0" layoutX="6.0" layoutY="7.0"
                                           pickOnBounds="true" fx:id="imageResultView" preserveRatio="true"/>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
